{
  "name": "Ionic-Monitoring",
  "version": "0.0.1",
  "author": "IoTize Solutions",
  "homepage": "http://www.iotize.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "setup": "ionic cordova plugin remove cordova-plugin-iotize-ble && npm i && ionic cordova plugin add @iotize/cordova-plugin-iotize-ble",
    "ios": "ionic cordova run ios",
    "android": "ionic cordova run android",
    "win": "ionic cordova run windows",
    "rebuild:android": "rm -rf node_modules plugins platforms && cordova platforms remove android && npm run setup && cordova platforms add android && cordova android build",
    "install:ble": "ionic cordova plugin add @iotize/cordova-plugin-iotize-ble@latest"
  },
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cli": "^6.2.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@ionic-native/core": "^4.17.0",
    "@ionic-native/http": "^4.17.0",
    "@ionic-native/nfc": "^4.18.0",
    "@ionic-native/splash-screen": "^4.17.0",
    "@ionic-native/status-bar": "^4.17.0",
    "@ionic/storage": "^2.2.0",
    "@iotize/cordova-plugin-iotize-ble": "^1.1.0-alpha.16",
    "@iotize/device-client.js": "0.0.1-alpha.57",
    "@iotize/device-com-websocket.js": "^0.0.1-alpha.24",
    "cordova-android": "7.1.4",
    "cordova-ios": "4.5.5",
    "cordova-plugin-add-swift-support": "^1.7.2",
    "cordova-plugin-advanced-http": "^2.0.1",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^1.2.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-windows": "5.0.0",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "phonegap-nfc": "1.0.3",
    "rxjs": "6.2.1",
    "rxjs-compat": "6.2.1",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.2.0",
    "@types/node": "^10.12.10",
    "typescript": "~2.7.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "platforms": [
      "ios",
      "windows",
      "android"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-add-swift-support": {},
      "cordova-plugin-advanced-http": {},
      "cordova-plugin-iotize-ble": {},
      "phonegap-nfc": {}
    }
  }
}